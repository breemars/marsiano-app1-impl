<VBox prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: lightgrey;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar style="-fx-background-color: lightgrey;" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Open Lists" />
            <MenuItem mnemonicParsing="false" text="Download Lists" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Show">
               <items>
                  <MenuItem mnemonicParsing="false" text="All Tasks" />
                  <MenuItem mnemonicParsing="false" text="Uncompleted Tasks" />
                  <MenuItem mnemonicParsing="false" text="Completed Tasks" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <CustomMenuItem hideOnClick="false" mnemonicParsing="false" text="Edit">
                    <content>
                      <CheckBox text="Edit task" textFill="#131313" />
                    </content>
                  </CustomMenuItem>
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Delete">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete task" />
                  <MenuItem mnemonicParsing="false" text="Clear List" />
               </items>
            </Menu>
      </menus>
    </MenuBar>
  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="551.0" prefWidth="815.0" style="-fx-border-color: white; -fx-border-width: 0px; -fx-background-color: white;" stylesheets="@style.css">
         <children>
            <TableView layoutX="108.0" layoutY="37.0" prefHeight="401.0" prefWidth="690.0" style="-fx-background-color: white; -fx-border-color: white; -fx-font-size: 20px; -fx-font-family: Corbel;" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="60.0" AnchorPane.rightAnchor="60.0" AnchorPane.topAnchor="30.0">
