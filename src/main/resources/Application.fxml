                <TableColumn id="tasksCol" prefWidth="568.0000228881836" text="tasks" />
                <TableColumn id="dateCol" minWidth="0.0" prefWidth="208.0" text="due date" />
              </columns>
            </TableView>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="49.0" layoutX="812.0" layoutY="423.0" stroke="WHITE" strokeType="INSIDE" width="44.0" AnchorPane.bottomAnchor="16.0" AnchorPane.rightAnchor="44.0" />
         </children>
      </AnchorPane>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: white;">
         <children>
            <TextField prefHeight="37.0" prefWidth="514.0" promptText="name" style="-fx-background-color: white; -fx-border-color: silver; -fx-border-radius: 10px;">
               <font>
                  <Font name="Corbel" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
            <Separator opacity="0.0" orientation="VERTICAL" prefHeight="37.0" prefWidth="25.0" />
            <DatePicker prefHeight="37.0" prefWidth="148.0" promptText="date" style="-fx-background-color: white; -fx-border-color: silver; -fx-font-size: 17px; -fx-font-family: Corbel;" />
            <Separator opacity="0.0" orientation="VERTICAL" prefHeight="37.0" prefWidth="25.0" />
            <Pane prefHeight="37.0" prefWidth="99.0">
               <children>
                  <Button layoutX="2.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="93.0" style="-fx-background-color: lightgrey;" text="Save Task">
                     <font>
                        <Font name="Corbel" size="17.0" />
                     </font></Button>
                  <Button layoutX="3.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="91.0" style="-fx-background-color: lightgrey;" text="New Task">
                     <font>
                        <Font name="Corbel" size="17.0" />
                     </font>
                  </Button>
               </children>
            </Pane>
         </children>
         <padding>
            <Insets bottom="20.0" left="55.0" right="50.0" top="10.0" />
         </padding>
      </HBox>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Status: Saved/Delete Mode/Edit Mode" HBox.hgrow="ALWAYS">
          <font>
            <Font name="OCR A Extended" size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="List Progress" textFill="$x4" HBox.hgrow="NEVER" />
            <ProgressBar prefHeight="16.0" prefWidth="167.0" progress="0.52">
               <effect>
                  <ColorAdjust brightness="0.3" contrast="0.04" hue="-0.13" saturation="-0.22" />
               </effect>
            </ProgressBar>
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
